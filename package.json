  "license": "MIT",
  "cordovaPlugins": [
    "cordova-plugin-statusbar",
    "cordova-plugin-splashscreen",
    "ionic-plugin-keyboard"
  ],
  "cordovaPlatforms": [
    {
      "platform": "android",
      "version": "",
      "locator": "android"
    }
  ],
  "devDependencies": {
    "bootstrap-sass": "^3.3.6",
    "chai": "^3.5.0",
    "command-line-args": "^2.1.4",
    "copy-webpack-plugin": "^1.1.1",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^2.7.2",
    "jasmine-core": "^2.3.4",
    "json-loader": "^0.5.4",
    "karma": "^0.13.19",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^0.2.2",
    "karma-coverage": "^0.5.3",
    "karma-jasmine": "^0.3.6",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.23",
    "karma-webpack": "1.7.0",
    "ncp": "^2.0.0",
    "node-sass": "^3.4.2",
    "phantomjs-polyfill": "0.0.1",
    "phantomjs-prebuilt": "^2.1.3",
    "postcss-loader": "^0.8.0",
    "precss": "^1.4.0",
    "protractor": "^3.0.0",
    "raw-loader": "^0.5.1",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "ts-loader": "^0.8.0",
    "tslint": "^3.3.0",
    "tslint-loader": "^2.1.0",
    "typedoc": "^0.3.12",
    "typescript": "^1.8.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.12",
    "yargs": "^3.32.0"
  },
  "scripts": {
    "start": "npm run server:dev",
    "server": "npm run server:dev",
    "server:dev": "webpack-dev-server --progress --profile --colors --display-error-details --display-cached --inline --content-base src/ --env dev",
    "server:prod": "http-server dist --cors",
    "build": "npm run build:dev",
    "build:dev": "rm -rf ./dist && webpack --config webpack.build.js --progress --profile --colors --display-error-details --display-cached --env prod",
    "build:prod": "webpack --config webpack.prod.config.js --progress --profile --colors --display-error-details --display-cached",
    "test": "karma start",
    "test:live": "karma start --auto-watch --no-single-run",
    "cordo": "npm run build:dev && cordova run android"
  },
  "dependencies": {
    "angular": "^1.5.0",
    "angular-animate": "^1.5.0",
    "angular-aria": "^1.5.0",
    "angular-jwt": "0.0.9",
    "angular-local-storage": "^0.2.2",
    "angular-material": "^1.0.5",
    "angular-messages": "^1.5.0",
    "angular-sanitize": "^1.4.9",
    "angular-toarrayfilter": "git+https://github.com/petebacondarwin/angular-toArrayFilter.git",
    "angular-translate": "^2.9.0",
    "angular-translate-loader-static-files": "^2.9.0",
    "angular-ui-router": "^0.2.17",
    "ionic-sdk": "github:driftyco/ionic",
    "lodash": "^4.3.0",
    "material-design-icons": "^2.1.3",
    "md5": "^2.0.0",
    "mdPickers": "github:alenaksu/mdPickers",
    "moment": "^2.11.2",
    "ng-cordova": "github:driftyco/ng-cordova"
  }
}
